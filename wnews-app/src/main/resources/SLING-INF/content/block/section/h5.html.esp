            <div class="col-md-12">
                <h5><%= currentNode.title %></h5>
                <%
                    var pars = currentNode.pars
                    for (var j in pars) {
                        out.write("<p>" + pars[j] + "</p>");
                    }

                    if (currentNode.sections) {
                        sling.include(currentNode.sections.path + ".h5.html");
                    }
                %>
            </div>
